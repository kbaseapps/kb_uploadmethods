      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "dynamic_dropdown",
      "dynamic_dropdown_options": {
        "data_source": "ftp_staging"
      }
    },
    {
      "id": "fastq_rev_staging_file_name",
      "optional": true,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "dynamic_dropdown",
      "dynamic_dropdown_options": {
        "data_source": "ftp_staging"
      }
    },
    {
      "id": "sequencing_tech",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        "Illumina"
      ],
      "field_type": "dropdown",
      "dropdown_options": {
        "options": [
          {
            "display": "Illumina",
            "value": "Illumina"
          },
          {
            "display": "PacBio CLR",
            "value": "PacBio CLR"
          },
          {
            "display": "PacBio CCS",
            "value": "PacBio CCS"
          },
          {
            "display": "IonTorrent",
            "value": "IonTorrent"
          },
          {
            "display": "NanoPore",
            "value": "NanoPore"
          },
          {
            "display": "Unknown",
            "value": "Unknown"
          }
        ]
      }
