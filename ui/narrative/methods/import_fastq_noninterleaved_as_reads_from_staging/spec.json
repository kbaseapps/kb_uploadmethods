      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "text",
      "text_options": {
        "validate_as": "float"
      }
    },
    {
      "id": "insert_size_mean",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "text",
      "text_options": {
        "validate_as": "float"
      }
    }
