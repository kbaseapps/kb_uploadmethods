      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": [
          "KBaseFile.SingleEndLibrary",
          "KBaseFile.PairedEndLibrary"
        ],
        "is_output_name": true
      }
    },
    {
      "id": "single_genome",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        1
