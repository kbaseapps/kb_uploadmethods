{
  "ver" : "1.0.0",
  "authors" : ["tgu2"],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : [ "active","upload" ],
  "widgets" : 
  {
    "input" : null,
    "output" : "kbaseReadsViewer"
  },
  "parameters" : 
  [ 
    {
      "id" : "fwd_staging_file_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {}
    },    
    {
      "id" : "rev_staging_file_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {}
    },
    {
      "id" : "sequencing_tech",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "Illumina" ],
      "field_type" : "dropdown",
      "dropdown_options": 
      {
        "options": 
        [
          {
              "display": "Illumina",
              "value": "Illumina"
          },
          {
              "display": "PacBio CLR",
              "value": "PacBio CLR"
          },
          {
              "display": "PacBio CCS",
              "value": "PacBio CCS"
          },
          {
              "display": "IonTorrent",
              "value": "IonTorrent"
          },
          {
              "display": "NanoPore",
              "value": "NanoPore"
          },
          {
              "display": "Unknown",
              "value": "Unknown"
          }
        ]
      }
    },
    {
      "id" : "name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : 
      {
        "valid_ws_types" : [ "KBaseFile.PairedEndLibrary" ],
        "is_output_name" : true
      }
    }, 
    {
      "id" : "insert_size_mean",
      "optional" : true,
      "advanced" : true,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "validate_as": "float"
      }
    }, 
    {
      "id" : "insert_size_std_dev",
      "optional" : true,
      "advanced" : true,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "validate_as": "float"
        }
    },
    {
      "id": "read_orientation_outward",
      "optional":true,
      "advanced":true,
      "allow_multiple":false,
      "default_values":[0],
      "field_type" : "checkbox",
      "checkbox_options":{
        "checked_value": 1,
        "unchecked_value": 0
        }
    },
    {
      "id": "single_genome",
      "optional":false,
      "advanced":false,
      "allow_multiple":false,
      "default_values":[1],
      "field_type" : "checkbox",
      "checkbox_options":{
        "checked_value": 1,
        "unchecked_value": 0
        }
    }
  ],
  "behavior" : 
  {
    "service-mapping" : 
    {
      "url" : "",
      "name" : "kb_uploadmethods",
      "method" : "upload_fastq_file",
      "input_mapping" : 
      [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter": "fwd_staging_file_name",
          "target_property": "fwd_staging_file_name"
        },
        {
          "input_parameter": "rev_staging_file_name",
          "target_property": "rev_staging_file_name"
        },
        {
            "input_parameter": "sequencing_tech",
            "target_property": "sequencing_tech"
        },
        {
          "input_parameter": "name",
          "target_property": "name"
        },
        {
          "input_parameter": "single_genome",
          "target_property": "single_genome"
        },
        {
          "constant_value": 1,
          "target_property": "interleaved"
        },
        {
          "input_parameter": "insert_size_mean",
          "target_property": "insert_size_mean"
        },
        {
          "input_parameter": "insert_size_std_dev",
          "target_property": "insert_size_std_dev"
        },
        {
          "input_parameter": "read_orientation_outward",
          "target_property": "read_orientation_outward"
        }
      ],
      "output_mapping" : 
      [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
          "service_method_output_path": [0,"obj_ref"],
          "target_property": "obj_ref",
          "target_type_transform": "resolved-ref"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
