{
  "ver" : "1.0.0",
  "authors" : ["slebras"],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : [ "inactive","assembly","upload" ],
  "widgets" : {
    "input" : null,
    "output" : "no-display"
  },
  "parameters" :
  [
    {
      "id" : "fasta_file",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "dynamic_dropdown",
      "dynamic_dropdown_options" : {
        "data_source": "ftp_staging"
      }
    },
    {
      "id" : "gff_file",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "dynamic_dropdown",
      "dynamic_dropdown_options" : {
        "data_source": "ftp_staging"
      }
    },
    {
      "id" : "genome_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" :
      {
        "valid_ws_types" : [ "KBaseMetagenomes.AnnotatedMetagenomeAssembly" ],
        "is_output_name" : true
      }
    },
    {
      "id" : "source",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "Other" ],
      "field_type" : "dropdown",
      "dropdown_options":{
      "options": [
        {
          "value": "EBI user",
          "display": "EBI"
        },
        {
          "value": "IMG user",
          "display": "IMG"
        },
        {
          "value": "JGI user",
          "display": "JGI"
        },
        {
          "value": "BGI user",
          "display": "BGI (Beijing Genomics)"
        },
        {
          "value": "Other",
          "display": "Other"
        }
      ]}
    },
    {
      "id": "release",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [ "" ],
      "field_type" : "text"
    },
    {
      "id": "genetic_code",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [ "" ],
      "field_type" : "text",
      "text_options" : {
        "validate_as": "int"
      }
    },
    {
      "id": "generate_missing_genes",
      "optional":true,
      "advanced":true,
      "allow_multiple":false,
      "default_values":["1"],
      "field_type" : "checkbox",
      "checkbox_options":{
        "checked_value": 1,
        "unchecked_value": 0
      }
    }
  ],
  "behavior" :
  {
    "service-mapping" :
    {
      "url" : "",
      "name" : "kb_uploadmethods",
      "method" : "upload_metagenome_fasta_gff_file",
      "input_mapping" :
      [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter": "fasta_file",
          "target_property": "fasta_file"
        },
        {
          "input_parameter": "gff_file",
          "target_property": "gff_file"
        },
        {
          "input_parameter": "genome_name",
          "target_property": "genome_name"
        },
        {
          "input_parameter": "source",
          "target_property": "source"
        },
        {
          "input_parameter": "release",
          "target_property": "release"
        },
        {
          "input_parameter": "genetic_code",
          "target_property": "genetic_code"
        },
        {
          "input_parameter": "generate_missing_genes",
          "target_property": "generate_missing_genes"
        }
      ],
      "output_mapping" :
      [
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
